.table-borderd > {
  border: 1px solid dimgrey;
  padding: 1px;
}

.table-borderd > tbody > tr > th {
  border: 1px solid dimgrey;
  padding: 2px;
}

.table-borderd > thead > tr > th {
  border: 1px solid dimgrey;
  padding: 2px;
}

.table-borderd > tbody > tr > td {
  border: 1px solid dimgrey;
  padding: 2px;
}

.table-borderd > thead > tr > td {
  border: 1px solid dimgrey;
  padding: 2px;
}

.status-label {
  color: darkblue;
  padding: 2px 6px;
  border-radius: 3px;
}

.fade-enter-active,
.fade-leave-active {
  transition: opacity 0.3s ease;
}

.fade-enter-from,
.fade-leave-to {
  opacity: 0;
}

.primary-link {
  padding: 2px 4px;
  color: #3a83d0;
  border: 1px solid #3a83d0;
  border-radius: 3px;
}

.success-link {
  padding: 2px 4px;
  color: #50d05b;
  border: 1px solid #50d05b;
  border-radius: 3px;
}

.warning-link {
  padding: 2px 4px;
  color: @warning-color;
  border: 1px solid @warning-color;
  border-radius: 3px;
}

.error-link {
  padding: 2px 4px;
  color: @error-color;
  border: 1px solid @error-color;
  border-radius: 3px;
}

.yellow-link {
  padding: 2px 4px;
  color: #6e2cd7;
  border: 1px solid #6e2cd7;
  border-radius: 3px;
}

.corner-border {
  background: linear-gradient(@cornerColor, @cornerColor) left top, linear-gradient(@cornerColor, @cornerColor) left top, linear-gradient(@cornerColor, @cornerColor) right top,
  linear-gradient(@cornerColor, @cornerColor) right top, linear-gradient(@cornerColor, @cornerColor) left bottom, linear-gradient(@cornerColor, @cornerColor) left bottom,
  linear-gradient(@cornerColor, @cornerColor) right bottom, linear-gradient(@cornerColor, @cornerColor) right bottom;
  background-repeat: no-repeat;
  background-size: 2px 14px, 14px 2px;
}

.warning-border {
  border: 1px solid red;
  border-radius: 4px;
  animation: twinkling2 5s infinite ease-in-out;
  animation-duration: 5s;
  animation-fill-mode: both;
}

@keyframes twinkling2 {
  0% {
    border-color: red;
  }
  50% {
    border-color: yellow;
  }
  100% {
    border-color: red;
  }
}

