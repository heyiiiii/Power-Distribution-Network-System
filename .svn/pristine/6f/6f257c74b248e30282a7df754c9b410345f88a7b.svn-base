}


.table-preview {
  table-layout: fixed;
  border-collapse: collapse;
  border-spacing: 0 0;
  width: 100%;
  font-size: inherit;
}

.table-preview > tbody > tr > td {
  border: 1px solid rgba(32, 86, 128, 0.58);
}

.table-preview > tbody > tr > th {
  border: 1px solid rgba(32, 86, 128, 0.58);
  font-weight: bold;
}

.fix-table {
  table-layout: fixed;
  word-break: break-all;
  word-wrap: break-word;
  border-collapse: collapse;
}

.fix-table,.fix-table tr th, .fix-table tr td {
  border: 1px solid rgba(32, 86, 128, 0.58);
  padding: 3px 4px;
}

.ivu-notice-desc {
  word-break: keep-all;
  word-wrap: break-word;
  white-space: normal;
  line-break: strict;
}

.brk {
  word-break: keep-all;
  word-wrap: break-word;
  white-space: normal;
  line-break: strict;
  min-width: 200px;
  max-width: 400px;
  text-align: center;
}

.role-p {
  width: 100%;
  padding-left: 0;
  padding-right: 0;
}
.role-p:not(:last-child) {
  border-bottom: 1px solid darkgrey;
}

.param-table {
  overflow: hidden;
  white-space: nowrap;
  border-spacing: 0 0;
}

.param-table > tfoot > tr > td {
  border: 1px solid #ccc;
  color: black;
  overflow: hidden;
  font-size: 14px;
  font-weight: bold;
}

.param-table > tfoot > tr > th {
  text-align: center;
  border: 1px solid #ccc;
  color: black;
  overflow: hidden;
  font-size: 14px;
  font-weight: bold;
}

.param-table > thead > tr > td {
  border: 1px solid #ccc;
  color: black;
  overflow: hidden;
  font-size: 14px;
  font-weight: bold;
}

.param-table > thead > tr > th {
  text-align: center;
  border: 1px solid #ccc;
  color: black;
  overflow: hidden;
  font-size: 14px;
  font-weight: bold;
}

.param-table > tbody > tr > td {
  border: 1px solid #ccc;
  color: black;
  overflow: hidden;
  font-size: 14px;
  font-weight: bold;
  padding: 2px 6px;
}

.param-table > tbody > tr > th {
  text-align: center;
  border: 1px solid #ccc;
  color: black;
  overflow: hidden;
  font-size: 14px;
  font-weight: bold;
  padding: 2px 4px;
}
