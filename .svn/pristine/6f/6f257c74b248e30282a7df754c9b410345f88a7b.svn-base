@import '~view-design/src/styles/index.less';

@body-background: #ffffff;
@gridGreen: #006569;
@focusColor: #4c93ee;
@hoverColor: #4474b8;
@cornerColor: #6e9de6;
@primary-color: #3a83d0;
@success-color: #50d05b;
@info-color: #349ae7;
@warning-color: #ff950e;
@error-color: #db4c4d;
@grey-color: rgba(255, 255, 255, 0.8);
@table-td-highlight-bg: #8dcdff;
@table-td-hover-bg: #a5ffbc;
@layout-header-padding: 0 5px;
@text-color: #121443;
@text-color-secondary: fade(@black, 75%);

body {
  margin: 0;
  padding: 0;
  background-color: #f5f7f9;
  font-family: Helvetica Neue,Helvetica,PingFang SC,Hiragino Sans GB,Microsoft YaHei,Arial,sans-serif;
  -webkit-font-smoothing: antialiased;
}

.ivu-input[disabled] {
  color: #38486a !important;
}

.ivu-select-selected-value {
  color: #38486a !important;
}

.ivu-select-dropdown-list {
  max-height: 420px;
}

.page-bg {
  background-image: linear-gradient(25deg, #dce0cc, #dce3df);
}

